{
    "manifest_version": 2,
    "name": "CoStricTorEx",
    "version": "1.0",
    "description": "An implementation of anonymous HSTS",
    "icons": {
      "48": "icon.png"
    },
    "permissions": [
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "tabs",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js","long_bundled.js"],
      "persistent": true
    },
    "browser_action": {
      "default_icon": {
        "48": "icon.png"
      },
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      "holding-page.html",
      "low-warn.html",
      "high-warn.html",
      "warning.js"
    ]
  }