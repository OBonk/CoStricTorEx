{
    "manifest_version": 2,
    "name": "CoStricTorEx",
    "version": "1.0",
    "description": "An implementation of anonymous HSTS",
    "icons": {
      "48": "icon.png"
    },
    "permissions": [
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_icon": {
        "48": "icon.png"
      }
    }
  }